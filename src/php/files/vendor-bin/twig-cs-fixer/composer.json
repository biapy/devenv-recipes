{
  "require-dev": {
    "vincentlanglet/twig-cs-fixer": "^3.7"
  },
  "scripts": {
    "post-install-cmd": [
      "@install-link"
    ],
    "install-link": [
      "mkdir --parent '../../vendor/bin' && test -e '../../vendor/bin/rector' || ln --symbolic '../../vendor-bin/rector/vendor/bin/rector' '../../vendor/bin/rector'"
    ]
  },
  "scripts-descriptions": {
    "install-link": "Install composer bin link"
  }
}
